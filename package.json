{
  "name": "lab-node-working-with-scopes",
  "version": "1.1.1",
  "description": "A set of example applications for that rely on the Auth0 VSCode Extension.",
  "license": "MIT",
  "author": "Auth0",
  "bugs": {
    "url": "https://github.com/auth0-training/labs-node-working-with-scopes/issues"
  },
  "homepage": "https://github.com/auth0-training/labs-node-working-with-scopes#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/auth0-training/labs-node-working-with-scopes.git"
  },
  "scripts": {
    "init": "concurrently \"npm:init:dependencies\"",
    "init:dependencies": "concurrently \"npm:api:dependencies\"  \"npm:web-app:dependencies\"",
    "api:start": "npm start --prefix ./src/api",
    "api:dependencies": "npm install --prefix ./src/api",
    "web-app:start": "npm start --prefix ./src/web-app",
    "web-app:dependencies": "npm install --prefix ./src/web-app"
  },
  "keywords": [
    "auth0",
    "oauth2",
    "authorization",
    "samples",
    "tools",
    "labs"
  ],
  "dependencies": {
    "concurrently": "^6.3.0",
    "eslint": "^8.0.1",
    "prettier": "^2.4.1"
  }
}
